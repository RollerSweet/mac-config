# Load Powerlevel10k theme if not handled by Oh My Zsh
source ~/powerlevel10k/powerlevel10k.zsh-theme

# Load Powerlevel10k config if available
[[ -f ~/.p10k.zsh ]] && source ~/.p10k.zsh

